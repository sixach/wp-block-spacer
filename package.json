{
	"name": "@sixach/wp-block-spacer",
	"version": "1.0.0",
	"description": "Spacer block for WordPress editor.",
	"keywords": [
		"blank",
		"margin",
		"divider",
		"separator"
	],
	"homepage": "https://github.com/sixach/sixa-wp-block-spacer#readme",
	"bugs": {
		"url": "https://github.com/sixach/sixa-wp-block-spacer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/sixa-wp-block-spacer.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"main": "build/index.js",
	"scripts": {
		"dev": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:php": "composer lint:php",
		"lint:wpcs": "composer lint:wpcs",
		"lint:wpcs-fix": "composer lint:wpcbf",
		"packages-update": "wp-scripts packages-update"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.css": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		]
	},
	"dependencies": {
		"@wordpress/block-editor": "^5.3.0",
		"@wordpress/blocks": "^8.0.0",
		"@wordpress/components": "^13.0.0",
		"@wordpress/compose": "^3.25.0",
		"@wordpress/data": "^4.27.0",
		"@wordpress/element": "^2.20.0",
		"@wordpress/i18n": "^3.19.0",
		"@wordpress/primitives": "^1.12.1"
	},
	"devDependencies": {
		"@sixach/wp-block-utils": "^1.0.0",
		"@wordpress/scripts": "16.1.0",
		"autoprefixer": "^10.2.6",
		"classnames": "2.3.1",
		"husky": "^6.0.0",
		"lint-staged": "11.0.0",
		"lodash": "^4.17.21",
		"postcss": "8.3.6",
		"postcss-custom-media": "^8.0.0",
		"postcss-discard-empty": "^5.0.1",
		"postcss-if-media": "^1.0.3",
		"postcss-nested": "5.0.5",
		"postcss-nested-ancestors": "2.0.0",
		"postcss-selector-replace": "1.0.2"
	}
}
