{
	"name": "@sixach/wp-block-spacer",
	"version": "1.0.0",
	"description": "Spacer block for WordPress editor.",
	"keywords": [
		"blank",
		"margin",
		"divider",
		"separator"
	],
	"homepage": "https://github.com/sixach/sixa-wp-block-spacer#readme",
	"bugs": {
		"url": "https://github.com/sixach/sixa-wp-block-spacer/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sixach/sixa-wp-block-spacer.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "sixa AG",
	"main": "build/index.js",
	"scripts": {
		"dev": "wp-scripts start",
		"build": "wp-scripts build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css-fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js-fix": "wp-scripts lint-js --fix",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:php": "composer lint:php",
		"lint:wpcs": "composer lint:wpcs",
		"lint:wpcs-fix": "composer lint:wpcbf",
		"packages-update": "wp-scripts packages-update",
		"prepare": "husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.css": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"npm run lint:js",
			"npm run format:js"
		]
	},
	"dependencies": {
		"@wordpress/block-editor": "^7.0.0",
		"@wordpress/blocks": "^11.0.0",
		"@wordpress/components": "^15.0.0",
		"@wordpress/compose": "^5.0.0",
		"@wordpress/data": "^6.0.0",
		"@wordpress/element": "^4.0.0",
		"@wordpress/i18n": "^4.2.1",
		"@wordpress/primitives": "^3.0.0",
		"prop-types": "^15.7.2"
	},
	"devDependencies": {
		"@sixach/wp-block-utils": "^1.0.1",
		"@wordpress/base-styles": "^3.6.0",
		"@wordpress/scripts": "17.1.0",
		"classnames": "2.3.1",
		"husky": "^7.0.1",
		"lint-staged": "11.1.2",
		"lodash": "^4.17.21"
	}
}
