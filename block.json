{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "attributes": {
        "backgroundColor": {
            "type": "string"
        },
        "customBackgroundColor": {
            "type": "string"
        },
        "customGradient": {
            "type": "string"
        },
        "gradient": {
            "type": "string"
        },
        "height": {
            "default": 100,
            "type": "number"
        },
        "visible": {
            "default": {
                "laptop": false,
                "mobile": false,
                "tablet": false,
                "widescreen": false
            },
            "type": "object"
        }
    },
    "category": "design",
    "description": "Block to add empty vertical space (full-width).",
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "keywords": [
        "sixa",
        "blank",
        "divider",
        "margin",
        "separator"
    ],
    "name": "sixa/spacer",
    "style": "file:./build/style-index.css",
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "html": false
    },
    "textdomain": "sixa-block-spacer",
    "title": "Spacer"
}